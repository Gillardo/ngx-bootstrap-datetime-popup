{
  "name": "ngx-bootstrap-datetime-popup",
  "version": "3.2.6",
  "description": "Datetime popup using ngx-bootstrap",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "license": "MIT",
  "keywords": [
    "angular",
    "angular4",
    "aot",
    "aot-compatible",
    "aot-compilation",
    "library",
    "ng",
    "timepicker",
    "datepicker",
    "ngx-bootstrap",
    "ngx-bootstrap-datetime-popup",
    "typescript"
  ],
  "author": {
    "name": "Darren Gillard",
    "email": "darren.gillard81@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gillardo/ngx-bootstrap-datetime-popup"
  },
  "bugs": {
    "url": "https://github.com/gillardo/ngx-bootstrap-datetime-popup/issues"
  },
  "url": "https://github.com/gillardo/ngx-bootstrap-datetime-popup#readme",
  "scripts": {
    "build": "npm run build:esm && npm run build:umd",
    "build:esm": "gulp inline-templates && npm run ngcompile",
    "build:esm:watch": "gulp build:esm:watch",
    "build:umd": "webpack --config webpack-umd.config.ts",
    "build:umd:watch": "npm run build:umd -- --watch",
    "build:watch": "concurrently --raw \"npm run build:umd:watch\" \"npm run build:esm:watch\"",
    "ci": "npm run lint && npm run test && npm run build && npm run docs",
    "clean:all": "npm run clean:tmp && rimraf node_modules",
    "clean:tmp": "rimraf coverage dist tmp docs",
    "codecov": "cat coverage/lcov.info | codecov",
    "docs": "compodoc -p tsconfig.json -d docs --disableCoverage --disablePrivate --disableProtected --disableInternal --disableLifeCycleHooks",
    "explorer": "source-map-explorer ./dist/index.umd.js",
    "gh-pages": "rimraf docs && npm run docs && gh-pages -d docs",
    "lint": "npm run tslint 'src/**/*.ts'",
    "ngcompile": "node_modules/.bin/ngc -p tsconfig-aot.json",
    "postversion": "git push && git push --tags",
    "prebuild": "rimraf dist tmp",
    "prebuild:watch": "rimraf dist tmp",
    "prepublishOnly": "npm run ci",
    "preversion": "npm run ci",
    "tslint": "tslint"
  },
  "dependencies": {
    "ngx-bootstrap": "^3.1.2",
    "rxjs-compat": "^6.3.3",
    "webpack-cli": "^3.1.2"
  },
  "peerDependencies": {
    "@angular/common": "^6.0.4",
    "@angular/core": "^6.0.4",
    "@angular/forms": "^4.0.0"
  },
  "devDependencies": {
    "@angular/common": "^7.1.1",
    "@angular/compiler": "^7.1.1",
    "@angular/compiler-cli": "^7.1.1",
    "@angular/core": "^7.1.1",
    "@angular/forms": "^7.1.1",
    "@angular/platform-browser": "^7.1.1",
    "@angular/platform-browser-dynamic": "^7.1.1",
    "@compodoc/compodoc": "^1.1.7",
    "@types/jasmine": "^3.3.1",
    "@types/karma": "^3.0.1",
    "@types/node": "^10.12.12",
    "@types/tapable": "^1.0.4",
    "@types/webpack": "^4.4.20",
    "@types/webpack-env": "^1.13.6",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "^5.2.1",
    "codecov": "^3.1.0",
    "codelyzer": "^4.5.0",
    "concurrently": "^4.1.0",
    "css-loader": "^1.0.1",
    "gh-pages": "^2.0.1",
    "gulp": "^3.9.1",
    "gulp-inline-ng2-template": "^5.0.1",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jasmine-core": "^3.3.0",
    "json-loader": "^0.5.7",
    "karma": "^3.1.3",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-jasmine": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^3.0.5",
    "node-sass": "^4.10.0",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.2",
    "rxjs": "^6.3.3",
    "sass-loader": "^7.1.0",
    "source-map-explorer": "^1.6.0",
    "to-string-loader": "^1.1.5",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.1.3",
    "webpack": "^4.27.0",
    "webpack-angular-externals": "^1.0.2",
    "webpack-rxjs-externals": "^2.0.0",
    "zone.js": "^0.8.26"
  }
}
